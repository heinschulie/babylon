{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**", ".svelte-kit/**", "dist/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"preview": {
			"cache": false,
			"persistent": true
		},
		"check": {
			"dependsOn": ["^check"]
		},
		"check:watch": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"format": {
			"cache": false
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"test:run": {
			"dependsOn": ["^test:run"]
		}
	}
}
